{
  "name": "cd-vue-component-starter",
  "version": "0.0.1",
  "description": "CodeDungeon Vue Component Starter",
  "main": "./dist/hello-world.min.js",
  "homepage": "https://github.com/mikeerickson/cd-vue-component-starter",
  "bugs": {
    "url": "https://github.com/mikeerickson/cd-vue-component-starter/issues",
    "email": "codedungeon@gmail.com"
  },
  "keywords": [],
  "author": "Mike Erickson <codedungeon@gmail.com> (https://github.com/mikeerickson)",
  "license": "MIT",
  "scripts": {
    "lint:scripts": "eslint \"./src/js/**/*\" || true",
    "lint:scripts:watch": "esw \"./src/js/**/*\" --watch",
    "lint:sass": "sass-lint \"./src/sass/*.s+(a|c)ss\" -q -v || true",
    "lint:sass:watch": "node ./scripts/sass-lint-watch.js",
    "build:dev": "node -r babel-register node_modules/.bin/webpack-dev-server --inline --watch --progress --config webpack.dev.config.js --port 5000",
    "build:prod": "node -r babel-register node_modules/.bin/webpack --progress --hide-modules --config webpack.prod.config.js",
    "build:prod:cli": "vue build src/js/HelloWorld.vue --lib --prod"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-runtime": "6.23.0",
    "cd-messenger": "2.7.16",
    "chokidar": "1.6.1",
    "css-loader": "0.26.1",
    "eslint": "3.16.1",
    "eslint-config-google": "0.7.1",
    "eslint-config-vue": "2.0.2",
    "eslint-plugin-html": "2.0.1",
    "eslint-plugin-vue": "2.0.1",
    "eslint-watch": "3.0.0",
    "execa": "0.6.0",
    "file-loader": "0.10.0",
    "isparta": "4.0.0",
    "isparta-loader": "2.0.0",
    "node-sass": "4.5.0",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.2",
    "sasslint-loader": "0.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "vue-html-loader": "1.2.3",
    "vue-loader": "11.1.0",
    "vue-style-loader": "2.0.0",
    "vue-template-compiler": "2.1.10",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "3.0.0"
  },
  "dependencies": {
    "vue": "2.1.10"
  }
}
